<div class="container" style="text-align: center; padding-bottom: 50px;">
  <h1 style="margin-bottom: 40px"><%= @user.name %>'s Apps</h1>
  <ul class="list-unstyled" id="apps">
  <% if @user.apps.empty? %>
  <p>This user hasn't created any public apps.</p>
  <% end %>
  <%= render partial: 'apps', locals: {apps: @user.apps} %>
</div>
